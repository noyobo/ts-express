{
  "name": "ts-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ttsc --build ./tsconfig.json",
    "tsc": "tsc --build ./tsconfig.json",
    "clear": "rm -rf ./dist",
    "dist": "npm run clear && npm run build",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet --fix",
    "start": "NODE_ENV=production node bin/www",
    "start:dev": "NODE_ENV=development node bin/www",
    "dev": "NODE_ENV=test nodemon",
    "watch-node": "nodemon bin/www",
    "test": "jest --forceExit --coverage --verbose"
  },
  "dependencies": {
    "@overnightjs/core": "1.6.11",
    "cookie-parser": "~1.4.3",
    "express": "~4.17.1",
    "hbs": "4.0.6",
    "http-errors": "1.7.3",
    "morgan": "1.9.1",
    "mysql2": "2.0.2",
    "sequelize": "5.21.3"
  },
  "devDependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.6",
    "@types/bluebird": "3.5.29",
    "@types/busboy": "0.2.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "2.8.6",
    "@types/debug": "4.1.5",
    "@types/decamelize": "3.0.3",
    "@types/express": "4.17.1",
    "@types/http-errors": "^1.6.3",
    "@types/jsonwebtoken": "8.3.5",
    "@types/morgan": "^1.7.37",
    "@types/multer": "1.3.10",
    "@types/node": "12.12.18",
    "@types/qs": "6.9.0",
    "@types/request": "2.48.3",
    "@types/request-promise-native": "1.0.17",
    "@types/sequelize": "4.28.8",
    "@types/sharp": "0.23.1",
    "@types/socket.io": "2.1.4",
    "@types/uuid": "3.4.6",
    "@types/validator": "12.0.1",
    "@types/xml2js": "0.4.5",
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "babel-eslint": "10.0.3",
    "debug": "4.1.1",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "gulp": "4.0.2",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "sequelize-cli": "5.5.1",
    "ts-node": "8.5.4",
    "tsconfig-paths": "3.9.0",
    "ttypescript": "1.5.8",
    "typescript": "3.7.3",
    "typescript-transform-paths": "1.1.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
